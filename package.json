{
  "name": "@tentkeep/clients",
  "version": "0.0.0",
  "description": "node interfaces for various clients",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && npm i && tsc --project tsconfig.json && npm run copy-dist-files && node scripts/cleanup-dist.js",
    "copy-dist-files": "cp -rL page-phantomjs-meta-grabber.js dist/",
    "debug": ". config/local.sh && ts-node --esm --project tsconfig.json ./tests/debug.ts",
    "test": "test",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "ship": "./scripts/ship.sh"
  },
  "repository": "https://github.com/tentkeep/tk-clients-js.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "clients"
  ],
  "author": "Tentkeep",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tentkeep/clients/issues"
  },
  "homepage": "https://github.com/tentkeep/clients#readme",
  "dependencies": {
    "@tentkeep/tentkeep": "^0.3.14",
    "got": "^12.4.1",
    "jsonwebtoken": "^9.0.0",
    "x2js": "^3.4.3",
    "xml2js": "^0.5.0"
  },
  "devDependencies": {
    "@types/node": "^18.8.4",
    "eslint": "^8.21.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}

  "bugs": {
    "url": "https://github.com/tentkeep/clients/issues"
  },
  "homepage": "https://github.com/tentkeep/clients#readme",
  "dependencies": {
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/node": "^18.8.4",
    "eslint": "^8.21.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
